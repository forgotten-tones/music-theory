// true left side many sharp
ASSERT_D(mah_is_enharmonic(NOTE(C, 0, MAH_OCTAVE_5), NOTE(B, 1, MAH_OCTAVE_4)), true);

// true equal side
ASSERT_D(mah_is_enharmonic(NOTE(D, 1, MAH_OCTAVE_2), NOTE(D, 1, MAH_OCTAVE_2)), true);

// true E F
ASSERT_D(mah_is_enharmonic(NOTE(F, -1, MAH_OCTAVE_3), NOTE(E, 0, MAH_OCTAVE_3)), true);

// true right side many flat
ASSERT_D(mah_is_enharmonic(NOTE(E, 2, MAH_OCTAVE_3), NOTE(A, -3, MAH_OCTAVE_3)), true);

// true both side accidental
ASSERT_D(mah_is_enharmonic(NOTE(G, 1, MAH_OCTAVE_7), NOTE(A, -1, MAH_OCTAVE_7)), true);

// true both side sharp
ASSERT_D(mah_is_enharmonic(NOTE(A, 4, MAH_OCTAVE_7), NOTE(B, 2, MAH_OCTAVE_7)), true);

// false different octave
ASSERT_D(mah_is_enharmonic(NOTE(G, 1, MAH_OCTAVE_3), NOTE(G, 1, MAH_OCTAVE_4)), false);

// false B C different octave
ASSERT_D(mah_is_enharmonic(NOTE(B, 1, MAH_OCTAVE_4), NOTE(C, 1, MAH_OCTAVE_5)), false);

// false left side many sharp
ASSERT_D(mah_is_enharmonic(NOTE(C, 4, MAH_OCTAVE_2), NOTE(F, 0, MAH_OCTAVE_2)), false);

// false both side accidental
ASSERT_D(mah_is_enharmonic(NOTE(F, -1, MAH_OCTAVE_3), NOTE(E, 2, MAH_OCTAVE_3)), false);

// false unison
ASSERT_D(mah_is_enharmonic(NOTE(A, -1, MAH_OCTAVE_3), NOTE(A, 0, MAH_OCTAVE_3)), false);