// natural
ASSERT_D(mah_query_acci(&KEY(MAJOR_KEY, 0, 0, NOTE(C, 0, MAH_OCTAVE_0), NOTE_K(
    0
)), MAH_F), 0);

// flat
ASSERT_D(mah_query_acci(&KEY(MINOR_KEY, 0, 0, NOTE(B, -1, MAH_OCTAVE_0), NOTE_K(
    NOTE(B, -1, MAH_OCTAVE_0),
    NOTE(E, -1, MAH_OCTAVE_0),
    NOTE(A, -1, MAH_OCTAVE_0),
    NOTE(D, -1, MAH_OCTAVE_0),
    NOTE(G, -1, MAH_OCTAVE_0)
)), MAH_D), -1);

// sharp
ASSERT_D(mah_query_acci(&KEY(MAJOR_KEY, 0, 0, NOTE(D, 1, MAH_OCTAVE_0), NOTE_K(
    NOTE(F, 2, MAH_OCTAVE_0),
    NOTE(C, 2, MAH_OCTAVE_0),
    NOTE(G, 1, MAH_OCTAVE_0),
    NOTE(D, 1, MAH_OCTAVE_0),
    NOTE(A, 1, MAH_OCTAVE_0),
    NOTE(E, 1, MAH_OCTAVE_0),
    NOTE(B, 1, MAH_OCTAVE_0)
)), MAH_G), 1);