// major sharp key
ASSERT_K(mah_get_key_sig(NOTE(A, 0, MAH_OCTAVE_0), MAJOR_KEY), KEY(MAJOR_KEY, 3, 3, NOTE(A, 0, MAH_OCTAVE_0), NOTE_K(
