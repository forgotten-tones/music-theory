ASSERT_K(mah_get_key_sig(NOTE(B, 0, MAH_OCTAVE_3), MINOR_KEY), KEY(MINOR_KEY, 2, 2, NOTE(B, 0, MAH_OCTAVE_0), NOTE_K(
    NOTE(F, 1, MAH_OCTAVE_0),
    NOTE(C, 1, MAH_OCTAVE_0)
