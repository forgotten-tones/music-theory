ASSERT_K(mah_get_key_sig(NOTE(D, -1, MAH_OCTAVE_5), MAJOR_KEY), KEY(MAJOR_KEY, -5, 5, NOTE(D, -1, MAH_OCTAVE_0), NOTE_K(
